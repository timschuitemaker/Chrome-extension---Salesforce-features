{
  "manifest_version": 2,
  "name": "Salesforce inject",
  "description": "This extension will append an 18 char id in a page layout.",
  "version": "1.0",
  "browser_action": {
    "default_icon": {
      "19": "assets/images/icon.png",
      "38": "assets/images/icon38.png"
    }
  },
  "content_scripts": [
    {
      "run_at" :"document_end",
      "matches": ["https://*.salesforce.com/*"],
      "js": ["app/jquery-3.1.1.min.js","app/script.js"]
    }
  ],
  "icons": { 
    "16": "assets/images/icon16.png",
    "48": "assets/images/icon48.png",
    "128": "assets/images/icon128.png" 
  }
}